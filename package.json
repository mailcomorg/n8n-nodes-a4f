{
  "name": "n8n-nodes-a4f-hosein",
  "version": "0.1.1",
  "description": "n8n node to interact with the A4F AI API",
  "keywords": [
    "n8n-community-node-package",
    "a4f",
    "ai",
    "llm"
  ],
  "license": "MIT",
  "homepage": "https://github.com/mailcomorg/n8n-nodes-a4f#readme",
  "author": {
    "name": "hosein ebrahimzadeh ",
    "email": "hoseinezci@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mailcomorg/n8n-nodes-a4f.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "npm@9.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/A4FApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/A4F/A4F.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.39",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-plugin-n8n-nodes-base": "^1.5.4",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "typescript": "~4.7.3"
  },
  "peerDependencies": {
    "n8n-core": "*",
    "n8n-workflow": "*"
  },
  "publishConfig": {
    "access": "public"
  }
} 