{
  "name": "@mailcomorg/n8n-nodes-a4f",
  "version": "0.1.0",
  "description": "n8n node to interact with the A4F AI API",
  "keywords": [
    "n8n-community-node-package",
    "a4f",
    "ai",
    "llm"
  ],
  "license": "MIT",
  "homepage": "https://github.com/mailcomorg/n8n-nodes-a4f#readme",
  "author": {
    "name": "hosein ebrahimzadeh ",
    "email": "hoseinezci@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mailcomorg/n8n-nodes-a4f.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/A4FApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/A4F/A4F.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.76",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "n8n-core": "1.11.1",
    "n8n-workflow": "^1.70.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "n8n-core": "*",
    "n8n-workflow": "*"
  },
  "publishConfig": {
    "access": "public"
  }
} 